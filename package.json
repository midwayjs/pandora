{
  "name": "pandora.js",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "glob": "^7.1.6",
    "lerna": "^3.22.1",
    "mwts": "^1.1.2",
    "nyc": "^13.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.9.9"
  },
  "scripts": {
    "postinstall": "lerna bootstrap --no-ci",
    "build": "lerna run build",
    "lint": "lerna exec mwts check",
    "lint:fix": "lerna exec mwts fix",
    "test": "lerna run test --concurrency=1 --stream",
    "cov": "lerna run cov --concurrency=1 --stream; node ./scripts/merge-coverages.js",
    "ci": "npm run build && npm run cov",
    "authors": "git log --format='%aN <%aE>' | sort -u > AUTHORS"
  },
  "files": [],
  "keywords": [
    "container",
    "runtime",
    "cluster",
    "service",
    "application"
  ],
  "homepage": "https://github.com/midwayjs/pandora",
  "repository": {
    "type": "git",
    "url": "https://github.com/midwayjs/pandora.git"
  },
  "engines": {
    "node": ">= 8.2.1"
  },
  "license": "MIT"
}
