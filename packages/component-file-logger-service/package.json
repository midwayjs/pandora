    "@types/node": "^12.0.0",
    "chai": "^3.5.0",
    "mm": "^2.1.0",
    "mocha": "^7.1.2",
    "nyc": "^13.1.0"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "dist"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
