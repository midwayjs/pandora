{
  "name": "@pandorajs/component-file-logger-service",
  "version": "2.2.0",
  "description": "Pandora 2 Component File Logger Service",
  "main": "build/src/ComponentFileLoggerService.js",
  "typings": "build/src/ComponentFileLoggerService.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "test": "NODE_ENV=unittest mocha",
    "cov": "nyc mocha",
    "ci": "npm run test"
  },
  "keywords": [
    "pandora"
  ],
  "license": "MIT",
  "devDependencies": {
    "@pandorajs/component-ipc-hub": "^2.1.2",
