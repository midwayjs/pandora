{
  "name": "@pandorajs/component-reporter-arms",
  "version": "3.0.1",
  "description": "Pandora 2 Component ARMS Reporter",
  "main": "build/src/ComponentArmsReporter.js",
  "typings": "build/src/ComponentArmsReporter.d.ts",
  "scripts": {
    "prebuild": "npm run protos:copy",
    "protos:copy": "cpx src/protos/opentelemetry/**/*.* build/src/protos/opentelemetry",
    "build": "tsc -p .",
    "test": "NODE_ENV=unittest mocha --trace-warnings",
    "cov": "nyc mocha",
    "ci": "npm run test"
  },
  "keywords": [
    "pandora"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/crc": "^3.4.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.0.0",
    "@types/sinon": "^9.0.5",
    "cpx": "^1.5.0",
    "mocha": "^7.1.2",
    "nyc": "^13.1.0",
    "sinon": "^9.0.3",
    "test-util": "3.0.1"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.proto",
    "build/src/**/*.d.ts"
  ],
  "dependencies": {
    "@grpc/proto-loader": "^0.5.4",
    "@opentelemetry/api": "^0.10.2",
    "@opentelemetry/core": "^0.10.2",
    "@opentelemetry/exporter-collector": "^0.10.2",
    "@opentelemetry/metrics": "^0.10.2",
    "@opentelemetry/resources": "^0.10.2",
    "@opentelemetry/tracing": "^0.10.2",
    "@pandorajs/component-decorator": "^3.0.1",
    "@pandorajs/component-indicator": "^3.0.1",
    "@pandorajs/component-metric": "^3.0.1",
    "@pandorajs/component-trace": "^3.0.1",
    "@pandorajs/semantic-conventions": "^3.0.1",
    "crc": "^3.8.0",
    "debug": "^4.1.1",
    "grpc": "^1.24.3"
  }
}
