{
  "name": "@pandorajs/component-logger",
  "version": "2.2.0",
  "description": "Pandora 2 Component Error Log",
  "main": "build/src/ComponentLogger",
  "typings": "build/src/ComponentLogger.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "test": "#SKIP NODE_ENV=unittest mocha --trace-warnings",
    "cov": "nyc mocha",
    "ci": "npm run test"
  },
  "keywords": [
    "pandora"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/extend": "^3.0.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^12.0.0",
    "chai": "^3.5.0",
    "mm": "^2.1.0",
    "mocha": "^3.5.0",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "dist",
      "src/types.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "files": [
    "build",
    "src"
  ],
  "dependencies": {
    "@pandorajs/component-decorator": "^2.1.2"
  }
}
